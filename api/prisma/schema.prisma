generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  name     String
  lastName String?
  email    String  @unique
  password String
}

model Product {
  id         Int      @id @default(autoincrement())
  name       String
  price      String
  discount   Int?
  console    Consoles
  categories String
  image      String
}

enum Consoles {
  PS4
  PS5
  XBOX_ONE
  NINTENDO_SWITCH
}
